<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.name, manage_event_path(event) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @events %>
<%= page_entries_info @events %>
